     gwc.output.replace('quick minded, ', 'quick minded (9/23), ');
     gwc.output.replace('intelligent, ', 'intelligent (10/23), ');
     gwc.output.replace('sharp, ', 'sharp (11/23), ');
     gwc.output.replace('bright, ', 'bright (12/23), ');
     gwc.output.replace('inventive, ', 'inventive (13/23), ');
     gwc.output.replace('ingenious, ', 'ingenious (14/23), ');
     gwc.output.replace('brilliant, ', 'brilliant (15/23), ');
